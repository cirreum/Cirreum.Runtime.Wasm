						<fieldset class="form-group">
							<legend class="mt-3">Radio Group</legend>
							<RadioGroup @bind-Value="@RadioOptionsValue" Name="optionsRadiosGroup">
								<Radio Id="@RadioOption1Id" Inline="@InlineSwitch" Value="@("option1")">
									<label for="@RadioOption1Id">Option one</label>
								</Radio>
								<Radio Inline="@InlineSwitch" Value="@("option2")" Label="Option two" />
								<Radio Inline="@InlineSwitch" Value="@("option3")" Label="Option three is disabled" Disabled="true" />
							</RadioGroup>
							<InvalidMessage For="() => RadioOptionsValue" />
						</fieldset>
						<fieldset class="form-group">
							<legend class="mt-3">Radio Group (No Labels)</legend>
							<RadioGroup @bind-Value="@RadioOptionsValue" Name="optionsRadios2">
								<Radio Inline="@InlineSwitch" Value="@("option1")" />
								<Radio Inline="@InlineSwitch" Value="@("option2")" />
								<Radio Inline="@InlineSwitch" Value="@("option3")" Disabled="true" />
							</RadioGroup>
							<InvalidMessage For="() => RadioOptionsValue" />
						</fieldset>
						<fieldset class="form-group">
							<legend class="mt-3">Checkboxes</legend>
							<Checkbox Inline="@InlineSwitch" Label="Default checkbox" @bind-Value="@RequiredCheck" />
							<InvalidMessage For="() => RequiredCheck" />
							<Checkbox Inline="@InlineSwitch" Label="Checked checkbox" Value="true" />
							<Checkbox Inline="@InlineSwitch" Label="Disabled checkbox" Disabled="true" />
							<Checkbox Inline="@InlineSwitch" Label="Diabled Checked checkbox" Value="true" Disabled="true" />
						</fieldset>
						<fieldset>
							<legend class="mt-3">Switches</legend>
							<Switch Inline="@InlineSwitch" Label="Default switch input" @bind-Value="@RequiredSwitch" />
							<InvalidMessage For="() => RequiredSwitch" />
							<Switch Inline="@InlineSwitch" Label="Default switch input checked" Value="true" />
							<Switch Inline="@InlineSwitch" Label="Unchecked + Disabled" Disabled=" true" name=" demoSwitch1" />
							<Switch Inline="@InlineSwitch" Label="Checked + Disabled" Value="true" Disabled="true" name="demoSwitch2" />
