<!-- TreeView
================================================== -->
<div class="bs-docs-section clearfix">
	<div class="row">
		<div class="col-lg-12">
			<div class="page-header">
				<h1 id="treeView">TreeView</h1>
			</div>

			<div class="bs-component">
				<div class="container-fluid">
					<div class="row mb-2">
						<div class="col-4">
							<fieldset>
								<legend>TreeView Options</legend>
								<Switch Label="Show Icons" @bind-Value="ShowIcons" />
								<Switch Label="Show Header" @bind-Value="ShowTreeViewHeader" />
								<Switch Label="Show Footer" @bind-Value="ShowTreeViewFooter" />
								<Switch Label="Show Node Headers" @bind-Value="ShowTreeViewNodeHeaders" />
								<div class="p-2 d-flex flex-wrap gap-1">
									<button class="btn btn-secondary btn-sm" @onclick="ResetFolders">Reload</button>
									<button class="btn btn-secondary btn-sm" @onclick="ClearFolders">Clear</button>
								</div>
								<div class="p-2 d-flex flex-wrap gap-1">
									<button class="btn btn-secondary btn-sm" @onclick="ExpandAll">Expand All</button>
									<button class="btn btn-secondary btn-sm" @onclick="CollapseAll">Collapse All</button>
									<button class="btn btn-secondary btn-sm" @onclick="SelectRandomNode">Select Random</button>
								</div>
								<div class="p-2">
									<Switch Label="Contains" @bind-Value="ContainsText" />
									<Switch Label="Include Selected" @bind-Value="IncludeSelectedNodes" />
									<Switch Label="Include Hidden" @bind-Value="IncludeHiddenNodes" />
									<Switch Label="Include Disabled" @bind-Value="IncludeDisabled" />
									<Switch Label="Case Sensitive" @bind-Value="CaseSensitive" />
									<div class="p-2">
										<TextSearch Label="select node" AriaLabel="select treeview node by name" OnSearchChanged="HandleTreeViewSearchTextChanged" />
									</div>
								</div>
							</fieldset>
						</div>
						<div class="col-8">
							<fieldset>
								<legend>DataGrid Options</legend>
								<Switch Label="Flush" @bind-Value="DGFlush" />
								<Switch Label="Pageable" @bind-Value="DGPageable" />
								<Switch Label="Responsive" @bind-Value="DGResponsive" />
							</fieldset>
						</div>
					</div>
					<div class="row">
						<div class="col-lg-4 col-6">
							<TreeView @ref=TreeviewRef
									  AutoExpandAll="false"
									  ShowIcons="@ShowIcons"
									  ShowHeader="@ShowTreeViewHeader"
									  ShowFooter="@ShowTreeViewFooter"
									  ShowNodeHeaders="@ShowTreeViewNodeHeaders"
									  Model="@Items"
									  OnNodeSelected="@SelectFolder"
									  OnNodeExpanded="@HandleNodeExpanded"
									  OnNodeCollapsed="@HandleNodeCollapsed"
									  OnNodeContextMenuItemSelected="@ContextMenuItemSelected"
									  Height="250px">
								<HeaderTemplate>
									<div class="hstack">
										<div class="me-auto">
											Root Nodes
										</div>
										<span class="ms-2 badge round-pill text-bg-primary">@Items.Count</span>
									</div>
								</HeaderTemplate>
								<FooterTemplate>
									<div class="hstack">
										<div class="me-auto">
											Selected: @SelectedFolderName
										</div>
										<span class="ms-2 badge round-pill text-bg-primary">@SelectedFolderData.Count</span>
									</div>
								</FooterTemplate>
								<NodeContextMenu>
									<MenuItem Label="Upload File" Value="Upload" />
									<MenuDivider />
									<MenuItem Label="Rename Folder" Value="Rename" />
									<MenuItem Label="Delete Folder" Value="Delete" />
									<MenuDivider />
									<MenuItem Label="Properties..." Value="Properties" />
									<MenuItem Label="Refresh" Value="Refresh" />
								</NodeContextMenu>
								<EmptyTemplate>
									<div class="text-center text-warning">Damn... No Items!</div>
								</EmptyTemplate>
							</TreeView>
						</div>
						<div class="col-lg-8 col-6">
							<DataGrid Responsive="@DGResponsive"
									  Pageable="@DGPageable"
									  Flush="@DGFlush"
									  Data="SelectedFolderData"
									  SelectedItems="SelectedData"
									  SelectionType="SelectionType.Multiple"
									  OnRefreshData="RefreshDataSourceAsync"
									  OnExportData="ExportDataGrid">
								<DataGridTitleTemplate>
									<div>
										<span>@SelectedFolderTitle</span>
									</div>
								</DataGridTitleTemplate>
								<DataGridColumn Field="@nameof(FileData.Name)" DefaultSortColumn="true" VisibleInColumnChooser="false" />
								<DataGridColumn Field="@nameof(FileData.ModifiedDate)" Header="Last Modified" />
								<DataGridColumn Field="@nameof(FileData.Type)" />
								<DataGridColumn Field="@nameof(FileData.FileSize)" Header="Size">
									@(context.IsFile ? Humanizer.ByteSize.FromBytes(context.FileSize).ToString() : "")
								</DataGridColumn>
							</DataGrid>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>


